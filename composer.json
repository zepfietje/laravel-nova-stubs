{
    "name": "zepfietje/laravel-nova-stubs",
    "description": "Clean versions of the stubs shipped with Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "stubs"
    ],
    "homepage": "https://github.com/zepfietje/laravel-nova-stubs",
    "license": "MIT",
    "authors": [
        {
            "name": "Zep Fietje",
            "homepage": "https://zepfietje.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^7.0",
        "laravel/nova": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "ZepFietje\\NovaStubs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZepFietje\\NovaStubs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZepFietje\\NovaStubs\\StubServiceProvider"
            ]
        }
    }
}
